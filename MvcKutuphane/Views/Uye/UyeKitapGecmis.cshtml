@using MvcKutuphane.Models.Entity
@model List<TBLHAREKET>
@{
    ViewBag.Title = "UyeKitapGecmis";
    Layout = "~/Views/Shared/Layout.cshtml";
}
<div style="margin-top:15px"></div>
<h3><em>Üye: @ViewBag.u1</em></h3>
<hr />
<div style="margin-top:15px"></div>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Kitap Adı</th>
            <th>Alış Tarihi</th>
            <th>İade Tarihi</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.TBLKITAP.AD</td>
                <td>@Convert.ToDateTime(x.ALISTARIH).ToString("dd/MM/yyyy")</td>
                <td>@Convert.ToDateTime(x.UYEGETIRTARIH).ToString("dd/MM/yyyy")</td>
            </tr>
        }
    </tbody>
</table>

